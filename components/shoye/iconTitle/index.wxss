/* 海报 */

.poster {
  display: flex;
  margin: 0 35rpx;
}

.posterOption {
  display: flex;
  flex-direction: column;
  height: 290rpx;
  width: 214rpx;
  text-align: center;
  border: 1px solid #f4f4f2;
  box-shadow: 0 0 10rpx 5rpx #f4f4f2;
  border-radius: 8rpx;
  margin-right: 11rpx;
}

.posterImages {
  width: 214rpx;
  height: 200rpx;
  margin-bottom: 34rpx;
}

.posterText {
  font-size: 15rpx;
  color: #4a4a48;
  display: inline-block;
}
